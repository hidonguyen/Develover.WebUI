<form id="form-supplier" class="form was-validated">
    <div class="form-row">
        <div class="col-sm-10">
            @Html.DeveloverTextBox(id: "Name", label: "Tên nhà cung cấp", placeholder: "Tên nhà cung cấp", validation: "required", validationMessage: "Tên nhà cung cấp không được để trống!")
        </div>
    </div>
    <div class="form-row">
        <div class="col-sm-10">
            @Html.DeveloverTextBox(id: "TaxCode", label: "Mã số thuế", placeholder: "Mã số thuế", validation: "required", validationMessage: "Mã số thuế không được để trống!")
        </div>
    </div>
    <div class="form-row">
        <div class="col-sm-10">
            @Html.DeveloverTextBox(id: "Phone", label: "Điện thoại", placeholder: "Điện thoại")
        </div>
    </div>
    <div class="form-row">
        <div class="col-sm-10">
            @Html.DeveloverTextBox(id: "Email", label: "Thư điện tử", placeholder: "Thư điện tử")
        </div>
    </div>
    <div class="form-row">
        <div class="col-sm-10">
            @Html.DeveloverTextArea(id: "Address", label: "Địa chỉ", placeholder: "Địa chỉ")
        </div>
    </div>
    <div class="form-row">
        <div class="col-sm-10">
            @Html.DeveloverTextArea(id: "Note", label: "Ghi chú", placeholder: "Ghi chú")
        </div>
    </div>
    <div class="form-row">
        <div class="col-sm-10">
            @Html.DeveloverSwitch("Status", "Sử dụng")
        </div>
    </div>
</form>
